@article{Claeyman2010,
abstract = {This paper presents an evaluation of a new linear parameterization valid$\backslash$nfor the troposphere and the stratosphere, based on a first order$\backslash$napproximation of the carbon monoxide (CO) continuity equation. This$\backslash$nlinear scheme (hereinafter noted LINCO) has been implemented in the 3-D$\backslash$nChemical Transport Model (CTM) MOCAGE (MOdele de Chimie Atmospherique$\backslash$nGrande Echelle). First, a one and a half years of LINCO simulation has$\backslash$nbeen compared to output obtained from a detailed chemical scheme output.$\backslash$nThe mean differences between both schemes are about +/- 25 ppbv (part$\backslash$nper billion by volume) or 15\% in the troposphere and +/- 10 ppbv or$\backslash$n100\% in the stratosphere. Second, LINCO has been compared to diverse$\backslash$nobservations from satellite instruments covering the troposphere$\backslash$n(Measurements Of Pollution In The Troposphere: MOPITT) and the$\backslash$nstratosphere (Microwave Limb Sounder: MLS) and also from aircraft$\backslash$n(Measurements of ozone and water vapour by Airbus in-service aircraft:$\backslash$nMOZAIC programme) mostly flying in the upper troposphere and lower$\backslash$nstratosphere (UTLS). In the troposphere, the LINCO seasonal variations$\backslash$nas well as the vertical and horizontal distributions are quite close to$\backslash$nMOPITT CO observations. However, a bias of similar to -40 ppbv is$\backslash$nobserved at 700 hPa between LINCO and MOPITT. In the stratosphere, MLS$\backslash$nand LINCO present similar large-scale patterns, except over the poles$\backslash$nwhere the CO concentration is underestimated by the model. In the UTLS,$\backslash$nLINCO presents small biases less than 2\% compared to independent MOZAIC$\backslash$nprofiles. Third, we assimilated MOPITT CO using a variational 3D-FGAT$\backslash$n(First Guess at Appropriate Time) method in conjunction with MOCAGE for$\backslash$na long run of one and a half years. The data assimilation greatly$\backslash$nimproves the vertical CO distribution in the troposphere from 700 to 350$\backslash$nhPa compared to independent MOZAIC profiles. At 146 hPa, the assimilated$\backslash$nCO distribution is also improved compared to MLS observations by$\backslash$nreducing the bias up to a factor of 2 in the tropics. This study$\backslash$nconfirms that the linear scheme is able to simulate reasonably well the$\backslash$nCO distribution in the troposphere and in the lower stratosphere.$\backslash$nTherefore, the low computing cost of the linear scheme opens new$\backslash$nperspectives to make free runs and CO data assimilation runs at high$\backslash$nresolution and over periods of several years.},
author = {Claeyman, M. and Att\'{\i}, J. L. and {El Amraoui}, L. and Cariolle, D. and Peuch, V. H. and Teyss\'{\i}dre, H. and Josse, B. and Ricaud, P. and Massart, S. and Piacentini, A. and Cammas, J. P. and Livesey, N. J. and Pumphrey, H. C. and Edwards, D. P.},
doi = {10.5194/acp-10-6097-2010},
file = {:home/alex/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Claeyman et al. - 2010 - A linear CO chemistry parameterization in a chemistry-transport model Evaluation and application to data assimi.pdf:pdf},
issn = {16807316},
journal = {Atmospheric Chemistry and Physics},
pages = {6097--6115},
title = {{A linear CO chemistry parameterization in a chemistry-transport model: Evaluation and application to data assimilation}},
volume = {10},
year = {2010}
}
