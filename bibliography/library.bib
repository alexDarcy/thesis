@article{Prather2008,
abstract = {One expectation when computationally solving an Earth system model is that a correct answer exists, that with adequate physical approximations and numerical methods our solutions will converge to that single answer. With such hubris, we performed a controlled numerical test of the atmospheric transport of CO(2) using 2 models known for accurate transport of trace species. Resulting differences were unexpectedly large, indicating that in some cases, scientific conclusions may err because of lack of knowledge of the numerical errors in tracer transport models. By doubling the resolution, thereby reducing numerical error, both models show some convergence to the same answer. Now, under realistic conditions, we identify a practical approach for finding the correct answer and thus quantifying the advection error.},
author = {Prather, Michael J. and Zhu, Xin and Strahan, Susan E. and Steenrod, Stephen D. and Rodriguez, Jose M.},
doi = {10.1073/pnas.0806541106},
file = {:home/alex/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Prather et al. - 2008 - Quantifying errors in trace species transport modeling.pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
month = dec,
number = {50},
pages = {19617--21},
pmid = {19066224},
title = {{Quantifying errors in trace species transport modeling.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2598731\&tool=pmcentrez\&rendertype=abstract},
volume = {105},
year = {2008}
}
